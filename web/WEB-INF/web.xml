<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>crearUsuario</servlet-name>
        <servlet-class>model.crearUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servlet_libro</servlet-name>
        <servlet-class>model.servlet_libro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Servlet_Prestamo</servlet-name>
        <servlet-class>model.Servlet_Prestamo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServletCrearLibro</servlet-name>
        <servlet-class>model.ServletCrearLibro</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>crearUsuario</servlet-name>
        <url-pattern>/crearUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servlet_libro</servlet-name>
        <url-pattern>/servlet_libro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Servlet_Prestamo</servlet-name>
        <url-pattern>/Servlet_Prestamo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServletCrearLibro</servlet-name>
        <url-pattern>/ServletCrearLibro</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
